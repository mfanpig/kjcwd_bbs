       //            xhFade();
       //        }
       //   },time)
       // }
       // xhFade();
       var sitv = setInterval(function(){
            if(curCount>maxCount){
              clearInterval(sitv);
            }
            list.eq(curCount).fadeIn(speed);
            curCount++;
       },time)
    }

