<link rel="stylesheet" type="text/css" href="template/time_9th_keji/src/list.css" />
<script src="template/time_9th_keji/src/js/jquery.superslide.js" type="text/javascript"></script>

<!--{ad/text/wp a_t}-->
<style id="diy_style" type="text/css"></style>
<div class="wp"> 
  <!--[diy=diy1]--><div id="diy1" class="area"></div><!--[/diy]--> 
</div>
<div class="tit_top cl"> 
        <!--{if ($_G['group']['allowpostarticle'] || $_G['group']['allowmanagearticle'] || $categoryperm[$catid]['allowmanage'] || $categoryperm[$catid]['allowpublish']) && empty($cat['disallowpublish'])}--> 
        <a href="portal.php?mod=portalcp&ac=article&catid=$cat[catid]" class="y post">{lang article_publish}</a> 
        <!--{/if}-->
        <h3>$cat[catname]</h3><div class="cl" style="float: left; min-width: 200px; min-height: 30px;"><!--[diy=diy_info]--><div id="diy_info" class="area"><div id="framep6c8DC" class="frame move-span cl frame-1"><div id="framep6c8DC_left" class="column frame-1-c"><div id="framep6c8DC_left_temp" class="move-span temp"></div><!--{block/139}--></div></div></div><!--[/diy]--></div> 
</div>
<div id="ct" class="ct2 wp inside_box cl">
  <div class="mn"> 
    <!--{ad/articlelist/mbm hm/1}--><!--{ad/articlelist/mbm hm/2}--> 
    <!--[diy=listcontenttop]--><div id="listcontenttop" class="area"></div><!--[/diy]-->
    <div class="bm">
      <!-- 文章列表 begin -->
      <div class="list_new Framebox cl"> 
        <!--{loop $list['list'] $value}--> 
        <!--{eval $highlight = article_title_style($value);}--> 
        <!--{eval $article_url = fetch_article_url($value);}-->
        
        <div class="news_list cl">
          <div class="cl"> 
            <!--{if $value[pic]}-->
            <div class="new_pic">
              <div class="bubba"><a href="$article_url" target="_blank"><img src="$value[pic]" alt="$value[title]"/></a>
                </div>
            </div>
            <!--{/if}-->
            <div class="new_body">
              <h2><a href="$article_url" target="_blank" $highlight>$value[title]</a> <!--{if $value[status] == 1}-->({lang moderate_need})<!--{/if}--></h2>
              <div class="meta"><!--{if $value[catname] && $cat[subs]}--><span class="meta-class"><a href="{$portalcategory[$value['catid']]['caturl']}">$value[catname]</a></span><!--{/if}--><span class="meta-date">$value[dateline]</span> 
                <!--{if $_G['group']['allowmanagearticle'] || ($_G['group']['allowpostarticle'] && $value['uid'] == $_G['uid'] && (empty($_G['group']['allowpostarticlemod']) || $_G['group']['allowpostarticlemod'] && $value['status'] == 1)) || $categoryperm[$value['catid']]['allowmanage']}--> 
                <span><a href="portal.php?mod=portalcp&ac=article&op=edit&aid=$value[aid]">{lang edit}</a></span> <span><a href="portal.php?mod=portalcp&ac=article&op=delete&aid=$value[aid]" id="article_delete_$value[aid]" onClick="showWindow(this.id, this.href, 'get', 0);">{lang delete}</a></span> 
                <!--{/if}--></div>
              <p>$value[summary]</p>
            </div>
          </div>
        </div>
        
        <!--{/loop}--> 
      </div>
      <!-- 文章列表 end --> 
      <!--[diy=listloopbottom]--><div id="listloopbottom" class="area"></div><!--[/diy]--> 
    </div>
    <!--{ad/articlelist/mbm hm/3}--><!--{ad/articlelist/mbm hm/4}--> 
    <!--{if $list['multi']}-->
    <div class="pgs cl">{$list['multi']}</div>
    <!--{/if}--> 
    
    <!--[diy=diycontentbottom]--><div id="diycontentbottom" class="area"></div><!--[/diy]--> 
    
  </div>
  <div class="sd pph">
    <div class="drag"> 
      <!--[diy=diyrighttop]--><div id="diyrighttop" class="area"></div><!--[/diy]--> 
    </div>
    
    <!-- 分类 -->
    <div class="list_box cl" style="margin: 0;">
    <!--{if $cat[subs]}-->
      <div class="tit01 cl" style="margin: 11px 0 20px 0;">
        <h2>{lang sub_category}</h2>
      </div>
      <div class="portal_sort Framebox2 cl" style="margin: 0 0 15px 0;">
        <ul>
          <!--{loop $cat[subs] $value}-->
          <li><a href="{$portalcategory[$value['catid']]['caturl']}">$value[catname]</a></li>
          <!--{/loop}-->
        </ul>
      </div>
      <!--{elseif $cat[others]}-->
      <div class="tit01 cl" style="margin: 11px 0 20px 0;">
        <h2>{lang category_related}</h2>
      </div>
      <div class="portal_sort Framebox2 cl" style="margin: 0 0 15px 0;">
        <ul>
          <!--{loop $cat[others] $value}-->
          <li><a href="{$portalcategory[$value['catid']]['caturl']}">$value[catname]</a></li>
          <!--{/loop}-->
        </ul>
      </div>
      <!--{/if}-->
    </div> 
    <!-- 推荐阅读 -->
    <div class="sbody cl" style="margin: 10px 0 0 0;"> 
      <!--[diy=sbody]--><div id="sbody" class="area"><div id="frameQQy7sf" class="frame move-span cl frame-1"><div id="frameQQy7sf_left" class="column frame-1-c"><div id="frameQQy7sf_left_temp" class="move-span temp"></div><!--{block/138}--></div></div><div id="frameES98zm" class="frame move-span cl frame-1"><div id="frameES98zm_left" class="column frame-1-c"><div id="frameES98zm_left_temp" class="move-span temp"></div><!--{block/142}--></div></div><div id="frameLqxtE7" class="frame move-span cl frame-1"><div id="frameLqxtE7_left" class="column frame-1-c"><div id="frameLqxtE7_left_temp" class="move-span temp"></div><!--{block/141}--></div></div><div id="frameMe2WMq" class="frame move-span cl frame-1"><div id="frameMe2WMq_left" class="column frame-1-c"><div id="frameMe2WMq_left_temp" class="move-span temp"></div><!--{block/143}--><!--{block/140}--><!--{block/144}--></div></div></div><!--[/diy]--> 
    </div>

    <div class="drag"> 
      <!--[diy=diy2]--><div id="diy2" class="area"></div><!--[/diy]--> 
    </div>
  </div>
</div>
<div class="wp mtn"> 
  <!--[diy=diy3]--><div id="diy3" class="area"></div><!--[/diy]--> 
</div>

